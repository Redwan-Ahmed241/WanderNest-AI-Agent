# Google Gemini API Key (required for LLM and embedding functionality)
GOOGLE_API_KEY="your_google_api_key_here"
# Documents folder path (where your documents are stored)
DOCUMENTS_FOLDER=<path_to_your_documents> e.g. e:/WanderNest-Agent/documents

# Chroma Configuration
CHROMA_PERSIST_DIR=<path_to_your_chroma_db> e.g. e:/WanderNest-Agent/chroma_db

# FastAPI Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
AUTO_RELOAD=true

